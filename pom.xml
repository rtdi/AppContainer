			<version>5.11.0.202103091610-r</version>
		</dependency>
		<dependency>
			<groupId>io.rtdi.appcontainer</groupId>
			<artifactId>appcontainerrealm</artifactId>
			<version>0.9.1</version>
			<scope>provided</scope>
		</dependency>
	</dependencies>
</project>
