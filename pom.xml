			<artifactId>activation</artifactId>
			<version>1.1.1</version>
		</dependency>
	    <dependency>
	      <groupId>org.apache.olingo</groupId>
	      <artifactId>odata-server-api</artifactId>
	      <version>4.7.0</version>
