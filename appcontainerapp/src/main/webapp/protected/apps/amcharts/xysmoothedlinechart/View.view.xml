<mvc:View height="100vh"
	controllerName="ui5app.Controller"
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns="sap.m"
	xmlns:ui5libs="ui5libs"
	>
	<ui5libs:controls.AppContainerPage pageTitle="XY Chart">
	    <ui5libs:amcharts.Chart
	    	id="dependencychart"
	    	width="100%"
	    	height="100%"
	    	>
			<ui5libs:layoutData>
				<FlexItemData growFactor="1" />
			</ui5libs:layoutData>
			<ui5libs:children>
				<ui5libs:amcharts.base.XYChart
					panY="false"
					wheelY="zoomX"
					layout="verticalLayout"
				>
				   	<ui5libs:customData>
				   		<ui5libs:controls.JsonContainer key="model" url="ui5app/data.json" />
				   	</ui5libs:customData>
					<ui5libs:xAxis>
						<ui5libs:amcharts.base.DateAxis
							componentData="{/}"
						>
							<ui5libs:baseInterval>
								<ui5libs:amcharts.base.ITimeInterval timeUnit="day" count="1" />
							</ui5libs:baseInterval>
						</ui5libs:amcharts.base.DateAxis>
					</ui5libs:xAxis>
					<ui5libs:yAxis>
						<ui5libs:amcharts.base.ValueAxis
							extraTooltipPrecision="1"
						>
						</ui5libs:amcharts.base.ValueAxis>
					</ui5libs:yAxis>
					<ui5libs:series>
						<ui5libs:amcharts.base.SmoothedXLineSeries
							name="Europe" 
							xAxisIndex="0" 
							yAxisIndex="0" 
							valueYField="europe" 
							valueXField="date"
							componentData="{/}"
						>
						</ui5libs:amcharts.base.SmoothedXLineSeries>
						<ui5libs:amcharts.base.SmoothedXLineSeries
							name="North America" 
							xAxisIndex="0" 
							yAxisIndex="0" 
							valueYField="namerica" 
							valueXField="date" 
							componentData="{/}"
						>
						</ui5libs:amcharts.base.SmoothedXLineSeries>
						<ui5libs:amcharts.base.SmoothedXLineSeries
							name="Asia" 
							xAxisIndex="0" 
							yAxisIndex="0" 
							valueYField="asia" 
							valueXField="date" 
							componentData="{/}"
						>
						</ui5libs:amcharts.base.SmoothedXLineSeries>
					</ui5libs:series>
				</ui5libs:amcharts.base.XYChart>
			</ui5libs:children>
	    </ui5libs:amcharts.Chart>
    </ui5libs:controls.AppContainerPage>
</mvc:View>
