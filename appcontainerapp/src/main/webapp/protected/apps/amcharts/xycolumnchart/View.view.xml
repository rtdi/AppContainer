<mvc:View height="100vh"
	controllerName="ui5app.Controller"
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns="sap.m"
	xmlns:ui5libs="ui5libs"
	>
	<ui5libs:controls.AppContainerPage pageTitle="XY Chart">
	    <ui5libs:amcharts.Chart
	    	id="dependencychart"
	    	width="100%"
	    	height="100%"
	    	>
			<ui5libs:layoutData>
				<FlexItemData growFactor="1" />
			</ui5libs:layoutData>
			<ui5libs:children>
				<ui5libs:amcharts.base.XYChart
				>
				   	<ui5libs:customData>
				   		<ui5libs:controls.JsonContainer key="model" url="ui5app/data.json" />
				   	</ui5libs:customData>
					<ui5libs:xAxis>
						<ui5libs:amcharts.base.CategoryAxis
							categoryField="category"
							componentData="{/}"
						>
						</ui5libs:amcharts.base.CategoryAxis>
					</ui5libs:xAxis>
					<ui5libs:yAxis>
						<ui5libs:amcharts.base.ValueAxis
						>
						</ui5libs:amcharts.base.ValueAxis>
					</ui5libs:yAxis>
					<ui5libs:series>
						<ui5libs:amcharts.base.ColumnSeries
							name="Series" 
							xAxisIndex="0" 
							yAxisIndex="0" 
							valueYField="value1" 
							categoryXField="category" 
							componentData="{/}"
						>
						</ui5libs:amcharts.base.ColumnSeries>
						<ui5libs:amcharts.base.ColumnSeries
							name="Series" 
							xAxisIndex="0" 
							yAxisIndex="0" 
							valueYField="value2" 
							categoryXField="category" 
							componentData="{/}"
						>
						</ui5libs:amcharts.base.ColumnSeries>
					</ui5libs:series>
				</ui5libs:amcharts.base.XYChart>
			</ui5libs:children>
	    </ui5libs:amcharts.Chart>
    </ui5libs:controls.AppContainerPage>
</mvc:View>
