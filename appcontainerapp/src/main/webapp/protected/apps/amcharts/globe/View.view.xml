<mvc:View height="100vh"
	controllerName="ui5app.Controller"
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns="sap.m"
	xmlns:ui5libs="ui5libs"
	>
	<ui5libs:controls.AppContainerPage pageTitle="Globe">
	    <ui5libs:amcharts.Chart
	    	id="dependencychart"
	    	width="100%"
	    	height="100%"
	    	>
			<ui5libs:layoutData>
				<FlexItemData growFactor="1" />
			</ui5libs:layoutData>
			<ui5libs:children>
				<ui5libs:amcharts.base.MapChart
					panX="rotateX"
					panY="rotateY"
					projection="geoOrthographic"
					paddingBottom="20"
					paddingTop="20"
					paddingLeft="20"
					paddingRight="20"
				>
				   	<ui5libs:customData>
				   		<ui5libs:controls.JsonContainer key="model" url="ui5app/data.json" />
				   	</ui5libs:customData>
					<ui5libs:series>
						<ui5libs:amcharts.base.MapPolygonSeries>
							<ui5libs:geoJSON>
								<ui5libs:amcharts.base.geodata.WorldLow/>
							</ui5libs:geoJSON> 
						</ui5libs:amcharts.base.MapPolygonSeries>
					</ui5libs:series>
				</ui5libs:amcharts.base.MapChart>
			</ui5libs:children>
	    </ui5libs:amcharts.Chart>
    </ui5libs:controls.AppContainerPage>
</mvc:View>
