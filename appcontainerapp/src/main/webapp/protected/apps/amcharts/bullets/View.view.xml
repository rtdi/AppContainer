<mvc:View height="100vh"
	controllerName="ui5app.Controller"
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns="sap.m"
	xmlns:ui5libs="ui5libs"
	>
	<ui5libs:controls.AppContainerPage pageTitle="Bullets Chart">
	    <ui5libs:amcharts.Chart
	    	id="dependencychart"
	    	width="100%"
	    	height="100%"
	    	>
			<ui5libs:layoutData>
				<FlexItemData growFactor="1" />
			</ui5libs:layoutData>
			<ui5libs:children>
				<ui5libs:amcharts.base.XYChart
					panY="false"
					wheelY="zoomX"
					layout="verticalLayout"
					maxTooltipDistance="0"
				>
				   	<ui5libs:customData>
				   		<ui5libs:controls.JsonContainer key="model" url="ui5app/data.json" />
				   	</ui5libs:customData>
					<ui5libs:xAxis>
						<ui5libs:amcharts.base.DateAxis
						>
							<ui5libs:baseInterval>
								<ui5libs:amcharts.base.ITimeInterval timeUnit="day" count="1" />
							</ui5libs:baseInterval>
						</ui5libs:amcharts.base.DateAxis>
					</ui5libs:xAxis>
					<ui5libs:rendererX>
						<ui5libs:amcharts.base.AxisRendererX
							minGridDistance="20"
						/>
					</ui5libs:rendererX>
					<ui5libs:yAxis>
						<ui5libs:amcharts.base.ValueAxis
							extraTooltipPrecision="1"
						>
						</ui5libs:amcharts.base.ValueAxis>
					</ui5libs:yAxis>
					<ui5libs:series>
						<ui5libs:amcharts.base.LineSeries
							name="Series" 
							xAxisIndex="0" 
							yAxisIndex="0" 
							valueYField="value" 
							valueXField="date"
							componentData="{/}"
						>
							<ui5libs:strokes>
								<ui5libs:amcharts.base.Graphics
									strokeWidth="2"
								>
								</ui5libs:amcharts.base.Graphics>
							</ui5libs:strokes>
							<ui5libs:bullets>
								<ui5libs:amcharts.base.Bullet
								>
									<ui5libs:sprite>
										<ui5libs:amcharts.base.Circle
											radius="5"
										>
										</ui5libs:amcharts.base.Circle>
									</ui5libs:sprite>
								</ui5libs:amcharts.base.Bullet>
							</ui5libs:bullets>
						</ui5libs:amcharts.base.LineSeries>
					</ui5libs:series>
				</ui5libs:amcharts.base.XYChart>
			</ui5libs:children>
	    </ui5libs:amcharts.Chart>
    </ui5libs:controls.AppContainerPage>
</mvc:View>
